pipeline{
    agent any

    stages {
        // week1-day2-abhishek-jenkins-pipeline-assignment
        stage('compile'){
            steps {
                echo 'compiling...'
            }
        }
        
        stage('unit test'){
            steps {
                echo 'running unit tests...'
            }
        }
        
        stage('build'){
            steps {
                echo 'creating jar...'
            }
        }
        
        stage('image'){
            steps {
                echo 'creating docker image...'
            }
        }

    }

}